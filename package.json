{
  "name": "slider",
  "version": "0.1.0",
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": {
    "name": "Max Str√ºbing",
    "email": "max.struebing@farbenmeer.de",
    "url": "https://maex.me"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,css}": [
      "npm run lint:fix",
      "npm run lint",
      "npm run lint:editorconfig",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "prettier-eslint $PWD/src/**/*",
    "lint:editorconfig": "ec",
    "lint:fix": "prettier-eslint --write $PWD/src/**/*"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.2.0",
    "react-spring": "8.0.27",
    "react-use-gesture": "6.0.14"
  },
  "devDependencies": {
    "commitlint": "8.2.0",
    "editorconfig-checker": "3.0.3",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "prettier-eslint-cli": "5.0.0"
  }
}

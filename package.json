{
	"name": "@farbenmeer/react-spring-slider",
	"version": "0.0.1-alpha.2",
	"main": "dist/index.js",
	"author": {
		"name": "Max Str√ºbing",
		"email": "max.struebing@farbenmeer.de",
		"url": "https://maex.me"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/farbenmeer/react-spring-slider.git"
	},
	"bugs": {
		"url": "https://github.com/farbenmeer/react-spring-slider/issues"
	},
	"homepage": "https://github.com/farbenmeer/react-spring-slider",
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -e",
			"pre-commit": "lint-staged"
		}
	},
	"xo": {
		"extends": "xo-react",
		"env": "browser"
	},
	"lint-staged": {
		"./**/*.{js,jsx,css}": [
			"npm run lint:fix",
			"npm run lint",
			"npm run lint:editorconfig",
			"git add"
		]
	},
	"scripts": {
		"build": "babel src/index.js --out-file dist/index.js",
		"watch": "babel --watch src --out-file dist/index.js",
		"lint": "xo --prettier $PWD/src/**/*",
		"lint:editorconfig": "ec",
		"lint:fix": "xo --prettier --fix $PWD/src/**/*",
		"prepublish": "rm -rf dist/ && mkdir dist && cp src/index.css dist/ && npm run build",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"peerDependencies": {
		"prop-types": "15.7.2",
		"react": "16.12.0",
		"react-dom": "16.12.0"
	},
	"dependencies": {
		"react-spring": "8.0.27",
		"react-use-gesture": "6.0.14"
	},
	"devDependencies": {
		"@babel/cli": "7.7.5",
		"@babel/core": "7.7.5",
		"@babel/preset-env": "7.7.6",
		"@babel/preset-react": "7.7.4",
		"@storybook/addon-actions": "5.2.8",
		"@storybook/addon-links": "5.2.8",
		"@storybook/addons": "5.2.8",
		"@storybook/react": "5.2.8",
		"babel-loader": "8.0.6",
		"commitlint": "8.2.0",
		"editorconfig-checker": "3.0.3",
		"eslint-config-xo-react": "0.20.0",
		"eslint-plugin-react": "7.17.0",
		"eslint-plugin-react-hooks": "2.3.0",
		"husky": "3.1.0",
		"lint-staged": "9.5.0",
		"parcel-bundler": "1.12.4",
		"prettier": "1.19.1",
		"prop-types": "15.7.2",
		"xo": "0.25.3"
	}
}
